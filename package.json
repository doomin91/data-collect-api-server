  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p .",
    "dev": "nodemon -e \".yaml .ts\" --watch \"src/**/*\" --exec node --loader ts-node/esm --experimental-specifier-resolution=node ./src/server.ts"
