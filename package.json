    "fs": "0.0.1-security",
    "node-schedule": "^2.1.0",
    "path": "^0.12.7",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@types/node-schedule": "^2.1.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
